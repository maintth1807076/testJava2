<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.MemberFormController">
    <children>
        <GridPane layoutX="1.0" layoutY="2.0" prefHeight="392.0" prefWidth="588.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <JFXTextField fx:id="txtusername" promptText="Username" />
                <JFXTextField fx:id="txtfullName" promptText="FullName" GridPane.columnIndex="1" />
                <JFXTextField fx:id="txtavatar" promptText="Avatar" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <JFXTextField fx:id="txtremainTime" promptText="RemainTime" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <JFXPasswordField fx:id="txtpassword" promptText="Password" GridPane.rowIndex="1" />
                <JFXButton fx:id="btnAdd" onAction="#add" text="Add" GridPane.rowIndex="3" />
                <JFXButton onAction="#reset" text="Reset" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            </children>
        </GridPane>
    </children>
</AnchorPane>
